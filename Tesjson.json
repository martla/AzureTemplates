{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "keyVaultName": {
        "type": "string"
      },
      "keyVaultRG": {
        "type": "string"
      },
      "keyVaultSecretName": {
        "type": "string"
      }
    },
    "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "nestedTemplate",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/martla/AzureTemplates/master/Tesjson.json",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "domainPassword": {
            "reference": {
              "keyVault": {
                "id": "[resourceId(subscription().subscriptionId,  parameters('keyVaultRG'), 'Microsoft.KeyVault/vaults', parameters('keyVaultName'))]"
              },
              "secretName": "[parameters('keyVaultSecretName')]"
            }
          }
        }
      }
    }],
    "outputs": {}
}
